*.png filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text
*.jpeg filter=lfs diff=lfs merge=lfs -text
*.img filter=lfs diff=lfs merge=lfs -imgfiles
*.bin filter=lfs diff=lfs merge-lfs -bin
*.elf filter=lfs diff=lfs merge-lfs -elf
*.dtbo filter=lfs diff=lfs merge-lfs -overlays
*.dtb filter=lfs diff=lfs merge-lfs -devicetree
*.a filter=lfs diff=lfs merge-lfs -staticlib
*.so* filter=lfs diff=lfs merge-lfs -dynamiclib
*.o filter=lfs diff=lfs merge-lfs -objfile
Image filter=lfs diff=lfs merge-lfs -kernelimage

